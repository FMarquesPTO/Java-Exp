      { "requires": "($enchanting_table_res = 1024x)", "$uv": [0, 816], "$uv_sz": [432, 76] },
      { "requires": "($enchanting_table_res = 2048x)", "$uv": [0, 1632], "$uv_sz": [864, 152] },
      { "requires": "($enchanting_table_res = 4096x)", "$uv": [0, 3264], "$uv_sz": [1728, 304] }
