      { "requires": "($enchanting_table_res = 2048x)", "$uv": [256, 1784], "$uv_sz": [128, 128] },
      { "requires": "($enchanting_table_res = 4096x)", "$uv": [512, 3568], "$uv_sz": [256, 256] }
    ]
  },

  "dust_level_3_locked@enchanting_table_jexp.dust_image_unselectable": {
    "$uv": [32, 239],
    "$uv_sz": [16, 16],
    "variables": [
      { "requires": "($enchanting_table_res = 512x)", "$uv": [64, 478], "$uv_sz": [32, 32] },
      { "requires": "($enchanting_table_res = 1024x)", "$uv": [128, 956], "$uv_sz": [64, 64] },
      { "requires": "($enchanting_table_res = 2048x)", "$uv": [256, 1912], "$uv_sz": [128, 128] },
      { "requires": "($enchanting_table_res = 4096x)", "$uv": [512, 3824], "$uv_sz": [256, 256] }
    ]
  },

  "enchanting_book_panel": {
    "type": "panel",
    "offset": [14, 10],
    "size": [40, 38],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      { "enchanting_book": {
          "type": "custom",
          "renderer": "enchanting_book_renderer",
          "layer": 13,
          "bindings": [ { "binding_name": "#open", "binding_type": "global" } ]
        }
      }
    ]
  },

  "slot": {
    "type": "grid",
    "grid_dimensions": [1, 1],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [18, 18]
  },

  "item_slot@enchanting_table_jexp.slot": {
    "offset": [14, 46],
    "collection_name": "enchanting_input_items",
    "controls": [
      { "item_slot@container_jexp.slot": {
          "grid_position": [0, 0],
          "$item_collt_nm": "enchanting_input_items"
        }
      }
    ]
  },

  "lapis_slot@enchanting_table_jexp.slot": {
    "offset": [34, 46],
    "collection_name": "enchanting_lapis_items",
    "controls": [
      { "lapis@container_jexp.slot": {
          "grid_position": [0, 0],
          "$item_collt_nm": "enchanting_lapis_items",
          "$focus_id_bdg_type": "none",
          "$focus_id": "enchanting_lapis"
        }
      }
    ]
  },

  "enchantments_grid_panel": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [60, 14],
    "size": [108, 57],
    "controls": [
      { "dust_panel@enchanting_table_jexp.dust_panel": { "layer": 10 } },
      { "enchantments_grid": {
          "type": "grid",
          "collection_name": "#enchant_buttons",
          "size": [108, 57],
          "anchor_from": "center",
          "anchor_to": "center",
          "grid_dimensions": [1, 3],
          "grid_item_template": "enchanting_table_jexp.enchant_button_panel",
          "layer": 2
        }
      }
    ]
  },

  "dust_template": {
    "type": "panel",
    "size": [18, 19],
    "controls": [
      { "dust_image_selectable@$dust_selected": {} },
      { "dust_image_unselectable$dust_unselected": {} }
    ]
  },

  "dust_panel": {
    "type": "grid",
    "grid_dimensions": [1, 3],
    "size": [18, 57],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [1, 0],
    "collection_name": "#enchant_buttons",
    "controls": [
      { "a@enchanting_table_jexp.dust_template": {
          "grid_position": [0, 0],
          "$dust_selected": "enchanting_table_jexp.dust_level_1",
          "$dust_unselected": "enchanting_table_jexp.dust_level_1_locked"
        }
      },
      { "b@enchanting_table_jexp.dust_template": {
          "grid_position": [0, 1],
          "$dust_selected": "enchanting_table_jexp.dust_level_2",
          "$dust_unselected": "enchanting_table_jexp.dust_level_2_locked"
        }
      },
      { "c@enchanting_table_jexp.dust_template": {
          "grid_position": [0, 2],
          "$dust_selected": "enchanting_table_jexp.dust_level_3",
          "$dust_unselected": "enchanting_table_jexp.dust_level_3_locked"
        }
      }
    ]
  },

  "enchant_button_panel": {
    "type": "panel",
    "size": [108, 19],
    "layer": 7,
    "controls": [
      { "enchantments_unselectable_bg@enchanting_table_jexp.enchantments_unselectable_bg": {} },
      { "unselectable_button@enchanting_table_jexp.unselectable_button": {} },
      { "selectable_button@enchanting_table_jexp.selectable_button": {} }
    ]
  },

  "base_button@enchanting.base_button": {
    "bindings": [
      { "binding_name": "$vis_bdg_nm",
        "binding_name_override": "#visible",
        "binding_collection_name": "#enchant_buttons",
        "binding_type": "collection"
      },
      { "binding_type": "collection_details",
        "binding_collection_name": "#enchant_buttons"
      }
    ]
  },

  "unselectable_button@base_button": {
    "$vis_bdg_nm": "#unselectable_button_visibility",
    "sound_volume": 0,
    "layer": 1,
    "button_mappings": [],
    "controls": [
      { "default@enchanting_table_jexp.enchantments_unselectable_bg": {} },
      { "hover@enchanting_table_jexp.enchantments_unselectable_bg": {
          "controls": [
            { "tooltip@container_jexp.item_tooltip": {
                "$item_collt_nm": "#enchant_buttons",
                "bindings": [
                  { "binding_name": "#hover_text",
                    "binding_collection_name": "#enchant_buttons",
                    "binding_type": "collection"
                  }
                ]
              }
            }
          ]
        }
      },
      { "pressed@enchanting_table_jexp.enchantments_unselectable_bg": {} },
      { "runes_panel@enchanting_table_jexp.runes_panel": { "$runes_color": "$enchanting_runes_fail_color" } },
      { "enchant_cost@enchanting_table_jexp.fail_cost": {} }
    ]
  },

  "selectable_button@base_button": {
    "$vis_bdg_nm": "#selectable_button_visibility",
    "$focus_ovrd_left": "enchanting_lapis",
    "$focus_ovrd_right": "FOCUS_OVERRIDE_STOP",
    "sound_name": "block.enchantment_table.use",
    "button_mappings": [
      { "from_button_id": "button.menu_select",
        "to_button_id": "button.menu_ok",
        "mapping_type": "pressed"
      },
      { "from_button_id": "button.menu_ok",
        "to_button_id": "button.menu_ok",
        "mapping_type": "focused"
      }
    ],
    "controls": [
      { "default@enchanting_table_jexp.enchantments_selectable_bg": {} },
      { "hover@enchanting_table_jexp.enchantments_selectable_hover_bg": {
          "controls": [
            { "tooltip@container_jexp.item_tooltip": {
                "$item_collt_nm": "#enchant_buttons",
                "bindings": [
                  { "binding_name": "#hover_text",
                    "binding_collection_name": "#enchant_buttons",
                    "binding_type": "collection"
                  }
                ]
              }
            }
          ]
        }
      },
      { "pressed@enchanting_table_jexp.enchantments_selectable_hover_bg": {} },
      { "runes_panel@enchanting_table_jexp.runes_panel": { "$runes_color": "$enchanting_runes_fail_color" } },
      { "enchant_cost@enchanting_table_jexp.success_cost": {} }
    ]
  },

  "runes_panel": {
    "type": "panel",
    "size": [78, 16],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [19, 1],
    "controls": [ { "runes@enchant_runes": {} } ]
  },

  "enchant_runes": {
    "type": "label",
    "color": "$runes_color",
    "$runes_color|default": "$enchanting_runes_color",
    "shadow": false,
    "layer": 1,
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "text": "#runes",
    "size": [78, 16],
    "font_type": "rune",
    "bindings": [
      { "binding_collection_name": "#enchant_buttons",
        "binding_name": "#runes",
        "binding_type": "collection"
      }
    ]
  },

  "enchant_cost": {
    "type": "label",
    "shadow": true,
    "offset": [-2, -1],
    "layer": 1,
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "text": "#cost",
    "bindings": [
      { "binding_name": "#cost",
        "binding_collection_name": "#enchant_buttons",
        "binding_type": "collection"
      }
    ]
  },

  "success_cost@enchanting_table_jexp.enchant_cost": { "color": "$enchanting_cost_success_text_color" },
  "fail_cost@enchanting_table_jexp.enchant_cost": { "color": "$enchanting_cost_fail_text_color" }
}